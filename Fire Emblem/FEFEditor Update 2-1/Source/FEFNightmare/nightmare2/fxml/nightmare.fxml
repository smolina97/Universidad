<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="mainContainer" prefHeight="450.0" prefWidth="260.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nightmare2.ViewController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
                  <MenuItem fx:id="openMenuItem" mnemonicParsing="false" onAction="#selectFile" text="Open" />
                  <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="saveMenuItem" disable="true" mnemonicParsing="false" onAction="#save" text="Save" />
            <MenuItem fx:id="saveAsMenuItem" disable="true" mnemonicParsing="false" onAction="#saveAs" text="Save As" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem fx:id="closeMenuItem" disable="true" mnemonicParsing="false" onAction="#close" text="Close" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem fx:id="aboutMenuItem" mnemonicParsing="false" onAction="#launchAboutWindow" text="About" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane id="mainAnchor" fx:id="anchorPane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="425.0" prefWidth="400.0" VBox.vgrow="ALWAYS">
         <children>
            <ToolBar fx:id="toolbar" disable="true" prefHeight="40.0" prefWidth="260.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
              <items>
                <Button mnemonicParsing="false" onAction="#save" prefHeight="25.0" prefWidth="42.0" text="Save" />
                  <Button layoutX="10.0" layoutY="13.0" mnemonicParsing="false" onAction="#saveAs" prefHeight="25.0" prefWidth="60.0" text="Save As" />
                  <Button layoutX="10.0" layoutY="13.0" mnemonicParsing="false" onAction="#close" prefHeight="25.0" prefWidth="45.0" text="Close" />
              </items>
            </ToolBar>
            <ListView fx:id="moduleList" disable="true" layoutY="40.0" prefHeight="355.0" prefWidth="260.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="40.0" />
         </children>
    </AnchorPane>
  </children>
</VBox>
